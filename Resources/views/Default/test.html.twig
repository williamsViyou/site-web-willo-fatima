Produits   enregistree en bd


{{ dump (produits) }}


{% for produit in produits %}
<section>
    <ul>
        <li>{{produit.id}}</li>
             <li>{{produit.nom}}</li>
             <li>{{produit.description}}</li>
             <li>{{produit.prix}}</li>
             <li>{{produit.disponible}}</li>
             <li>{{produit.categorie}}</li>
         
             <li>{{produit.tva}}</li>
    </ul>
</section>
        
{% endfor %}